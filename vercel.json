{
  "builds": [
    {
      "src": "api/football.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/pollitamayo/api/football",
      "destination": "/api/football"
    },
    {
      "source": "/pollitamayo/(.*)",
      "destination": "/$1"
    }
  ]
}