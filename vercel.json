{
  "rewrites": [
    {
      "source": "/pollitamayo/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/pollitamayo/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/football",
      "dest": "/api/football.ts"
    }
  ]
}