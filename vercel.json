{
  "rewrites": [
    {
      "source": "/pollitamayo/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/pollitamayo/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "functions": {
    "api/football.cjs": {
      "runtime": "@vercel/node@2.0.0"
    }
  }
}